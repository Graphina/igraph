> library(igraph)
> 
> ## Dense
> 
> I <- matrix(sample(0:1, 35, replace=TRUE, prob=c(3,1)), nc=5)
> g <- graph.incidence(I)
> I2 <- get.incidence(g)
> all(I==I2)
[1] TRUE
> all(rownames(I2) == 1:7)
[1] TRUE
> all(colnames(I2) == 8:12)
[1] TRUE
> 
